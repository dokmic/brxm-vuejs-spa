{
  "name": "@bloomreach/example-vuejs-csr",
  "version": "0.1.0",
  "private": true,
  "description": "Example Vue.js SPA for Bloomreach Experience",
  "author": "Michael Dokolin <mikhail.dokolin@bloomreach.com>",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "^14.0",
    "core-js": "^3.3",
    "vue": "^2.6",
    "vue-class-component": "^7.0",
    "vue-property-decorator": "^8.3",
    "vue-router": "^3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0",
    "@vue/cli-plugin-eslint": "^4.0",
    "@vue/cli-plugin-router": "^4.0",
    "@vue/cli-plugin-typescript": "^4.0",
    "@vue/cli-service": "^4.0",
    "@vue/eslint-config-airbnb": "^4.0",
    "@vue/eslint-config-typescript": "^4.0",
    "eslint": "^5.16",
    "eslint-plugin-vue": "^5.0",
    "typescript": "^3.5",
    "vue-template-compiler": "^2.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "rules": {
      "object-curly-newline": [0]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
